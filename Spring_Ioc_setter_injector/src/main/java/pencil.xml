<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
            
            
            <bean id = "mypencil" class="org.jsp.setter_injector.Pencil" init-method="load" destroy-method="unload">
            <property name="brand" value="Apsara"></property>
            <property name="price" value="5"></property>
            <property name="color" value="Silver"></property>
            
            </bean>
    

</beans>

